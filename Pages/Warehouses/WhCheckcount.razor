    protected override async Task OnInitializedAsync()
    {
        WhService objCS = new WhService();

        //////// Intial Menu and Privilege//////
        resultReturn = await Task.Run(() => OpjUserService.InsertMenu(mPagename, mPagedesc));
        resultReturn = await Task.Run(() => OpjUserService.InsertPrivilege(mPagename));
        ////////////////////////////////////////
        ///
        await GetUserAthu();
        this.InsertAuditrial("View", pageheader);
        Roles = await Task.Run(() => OpjUserService.GetUserRole(mPagename, long.Parse(sGroupID)));

        cOld = Elements.Count();
        Elements = await Task.Run(() => objCS.GetPicklist(mPalletcode));
        cNew = Elements.Count();
    }

    async Task GetUserAthu()
    {
        var authState = await AuthenticationStateProvider.GetAuthenticationStateAsync();
        var user = authState.User;
        sUsername = user.Identity.Name;

        sGroupID = user.Claims.FirstOrDefault(
        c => c.Type == ClaimTypes.Role)?.Value;

        sUserID = user.Claims.FirstOrDefault(
            c => c.Type == ClaimTypes.Actor)?.Value;
    }

    private void InsertAuditrial(String actdesc, String munname)
    {
        bool bRet = UserlogService.InsertAudittrial(actdesc, munname, long.Parse(sUserID));
    }

    private bool FilterFunc(Sap_StoreoutInfo element)
    {
        if (string.IsNullOrWhiteSpace(searchString))
            return true;
        if (element.Pallet_No.Contains(searchString, StringComparison.OrdinalIgnoreCase))
            return true;
        if (element.Su_No.Contains(searchString, StringComparison.OrdinalIgnoreCase))
            return true;
        if (element.Item_Code.Contains(searchString, StringComparison.OrdinalIgnoreCase))
            return true;
        if ($"{element.Item_Name}".Contains(searchString))
            return true;
        return false;
    }

    async Task DoTheThing(KeyboardEventArgs eventArgs)
    {
        if (eventArgs.Key == "Enter")        // fire on enter
        {
            bool? result = await CheckRoleApprove();
            if (result == true)
            {
                string pallettemp = stringputkey;
                await Task.Run(() => setstr(pallettemp));
                this.stringputkey = null;
                this.mPackid = null;
            }
            this.StateHasChanged();
        }
    }

    async Task setstr(string str)
    {
        if (mPalletcode.Length <= 0)
        {

            Boolean bValidate = false;
               mPalletcode = str.ToUpper();
            //bValidate = await Task.Run(() => ObjMasService.ValidateMasterpallet(str.ToUpper()));
              Elements = await Task.Run(() => ObjWhService.GetCountlist(mPalletcode));

            if (Elements.Count()>0)
                bValidate = true;

            if (bValidate)
            {
                mPalletcode = str.ToUpper();
                msgAction = "Enter TagNo";
                elevation = 1;
                Snackbar.Add("Enter TagNo", Severity.Normal);

                //Elements = await Task.Run(() => ObjWhService.GetCountlist(mPalletcode));

                this.InsertAuditrial("Start Counting TagNo : [" + mPalletcode + "]", pageheader);

            }
            else
            {
                mPalletcode = "";
                elevation = 4;
                msgAction = "TagNo find not found";
                Snackbar.Add("TagNo find not found", Severity.Warning);

            }
        }
        else
        {
            if (str.ToUpper() == mPalletcode)
            {
                if (Elements.ToList().Count > 0)
                {

                    var query = Elements.First(x => x.Su_No == mPalletcode);
                    string PalletCode = query.Pallet_No;
                    string retMsg = "";

                    bool bCheckCom = ObjOutboundService.UpdateStockOut(PalletCode,ref retMsg);
                    if (bCheckCom)
                    {
                        elevation = 2;
                        msgAction = "Counting successfully";

                        //ObjErpApiService.SetMappedPallet(mPalletcode);

                        // Set Update Count
                        bool bCheck = ObjWhService.UpdateCount(Elements.ToList());
                        this.InsertAuditrial("Update Counting TagNo : [" + mPalletcode + "]", pageheader);
                        // Set SAP Complete
                        if (bCheck)
                        {
                            //bCheck = ObjWhService.SapComplete(mPalletcode);
                            bCheck = true;

                            if (bCheck)
                            {
                                //bCheck = ObjWcsService.CreateCommandMC("G04", 88);
                                mPalletcode = "";
                                Snackbar.Add("Audit successfully", Severity.Success);

                                this.InsertAuditrial("Comfirm Counting TagNo : [" + mPalletcode + "]", pageheader);

                                Elements = await Task.Run(() => ObjWhService.GetCountlist(mPalletcode));

                            }
                            else
                            {
                                Snackbar.Add("Comfirm Error", Severity.Error);
                            }
                        }
                        else
                        {
                            Snackbar.Add("Adjust Stick Error", Severity.Error);
                        }
                    }
                    else
                    {
                        Snackbar.Add("Complete Error : " + retMsg, Severity.Error);
                    }
                    
                }
                else
                {
                    Snackbar.Add("Please enter TagNo", Severity.Info);
                }
            }
            else
            {
                mPackid = str;
                MapPalletItem = null;

                // find packid from list
                //var allCorrect = Elements.Where(a => a.Su_No.ToUpper() == str.ToUpper()).FirstOrDefault();

                var iCorrect = Elements.ToList().FindIndex(a => a.Su_No.ToUpper() == str.ToUpper());

                //----------------------

                if (iCorrect >= 0)
                {
                    Elements.ToList()[iCorrect].Bcount = true;
                    Elements.ToList()[iCorrect].Movement_Type = "CYC";
                    Elements.ToList()[iCorrect].Request_Qty = 0;
                    Elements.ToList()[iCorrect].Update_By = sUsername;
                    this.InsertAuditrial("Counting TagNo: [" + mPackid + "] is OK", pageheader);

                    Snackbar.Add("Counting TagNo: [" + mPackid + "] is OK", Severity.Success);
                }
                else
                {
                    Snackbar.Add("Find not found TagNo please enter again", Severity.Warning);
                }

            }
        }
    }

